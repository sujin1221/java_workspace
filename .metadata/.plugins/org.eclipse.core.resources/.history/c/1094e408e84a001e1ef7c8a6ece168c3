package Lotto;

import java.util.Scanner;

public class LottoE extends Lotto{
	Lotto[] cl = new Lotto[6]; //나의로또번호
	Lotto[] il = new Lotto[7];; //당첨로또번호
	//로또번호 자동 생성한 것을 배열에 넣기
	public static void createLottoAuto(int arr[]) {
		//중복없이 채우기
	   for(int i=0; i<arr.length; i++) {
		   int r = random();
		   for(int j=0 ; j<i ; j++) {
		   if(arr[i] != r) { //없다면..
			   arr[i] = r;
		   } else {
			   //있다면..
			  i--; 
		   }
	   }
		}
	}
	//당첨번호 자동 생성한 것을 배열에 넣기
	public void insertLottoAuto(int arr[]) {
		//중복없이 채우기
	   for(int i=0; i<arr.length; i++) {
		   int r = random1();
		   for(int j=0 ; j<i ; j++) {
		   if(arr[i] != r) { //없다면..
			   arr[i] = r;
		   } else {
			   //있다면..
			  i--; 
		   }
	   }
		}
	}
