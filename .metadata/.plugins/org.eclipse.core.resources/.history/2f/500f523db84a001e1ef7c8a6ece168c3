package studentmanager;

public class student_연습 {
	//멤버변수
	private int classNum = 12345; //학번
	private String studentName = "이름"; //학생 이름
	private String personNum = "000000-0000000"; //주민
	private String hakbu = "학부"; //학부
	private String major = "학과"; //학과
	private Subject[] s = new Subject[5]; //과목 5개를 배열로 저장
	int cnt = 0; //변수 카운트용
	//생성자
	public student_연습() {}
	public student_연습(String studentName) {
		this.studentName = studentName;
	}
	public student_연습(int classNum, String studentName, String personNum, String hakbu, String major, Subject[] s,
			int cnt) {
		super();
		this.classNum = classNum;
		this.studentName = studentName;
		this.personNum = personNum;
		this.hakbu = hakbu;
		this.major = major;
		this.s = s;
		this.cnt = cnt;
	}
	//getter setter
	
	
	
	
	
	
	
	
	
	
}
