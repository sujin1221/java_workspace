package studentmanager;

import java.util.Scanner;

/* 과목클래스 : 한 과목의 기본정보를 나타내는 클래스
 * - 과목코드 과목명 학점 시수 교수명 학기 시간표
 * - 멤버변수 선언, 생성자, getter setter, 그 외 필요한 메서드
 * => toString, print 메서드
 * */
public class Subject{
	public String subjectName;
	public int subjectCode;
	public int jumsu;
	public String sisu;
	public String teacherName;
	public int hakgi;
	public String siganpyo;
	
	public Subject() {}
	public Subject(String subjectName, String teacherName) {
		this.subjectName = subjectName;
		this.teacherName = teacherName;
	}
	public Subject(String subjectName, int subjectCode, int jumsu, String sisu, String teacherName, int hakgi,
			String siganpyo) {
		super();
		this.subjectName = subjectName;
		this.subjectCode = subjectCode;
		this.jumsu = jumsu;
		this.sisu = sisu;
		this.teacherName = teacherName;
		this.hakgi = hakgi;
		this.siganpyo = siganpyo;
	}
	public void subPrint() {
		System.out.println("과목코드: "+subjectCode+",과목명: "+subjectName+",학점: "+jumsu+",시수: "+sisu+",교수명: "+teacherName+",학기: "+hakgi+",시간표: "+siganpyo);
	}
	
	public String getSubjectName() {
		return subjectName;
	}
	public void setSubjectName(String subjectName) {
		this.subjectName = subjectName;
	}
	public int getSubjectCode() {
		return subjectCode;
	}
	public void setSubjectCode(int subjectCode) {
		this.subjectCode = subjectCode;
	}
	public int getJumsu() {
		return jumsu;
	}
	public void setJumsu(int jumsu) {
		this.jumsu = jumsu;
	}
	public String getSisu() {
		return sisu;
	}
	public void setSisu(String sisu) {
		this.sisu = sisu;
	}
	public String getTeacherName() {
		return teacherName;
	}
	public void setTeacherName(String teacherName) {
		this.teacherName = teacherName;
	}
	public int getHakgi() {
		return hakgi;
	}
	public void setHakgi(int hakgi) {
		this.hakgi = hakgi;
	}
	public String getSiganpyo() {
		return siganpyo;
	}
	public void setSiganpyo(String siganpyo) {
		this.siganpyo = siganpyo;
	}
	
	
	

	

}
