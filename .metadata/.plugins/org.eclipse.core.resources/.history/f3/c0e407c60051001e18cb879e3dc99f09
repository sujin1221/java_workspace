package safe;

public class word {
	//단어, 의미
	//정렬 => comparator 구현 객체
	//기존 클래스 + comparable 구현
	//정렬(equals 비교하여 정렬)
	//어떤값을 기준으로 정렬?
	private String word;
	private String mean;
	
	public word() {}
	public word(String word, String mean) {
		this.word = word;
		this.mean = mean;
	}
	public String getWord() {
		return word;
	}
	public void setWord(String word) {
		this.word = word;
	}
	public String getMean() {
		return mean;
	}
	public void setMean(String mean) {
		this.mean = mean;
	}
	@Override
	public String toString() {
		return word + ":" + mean;
	}

}
