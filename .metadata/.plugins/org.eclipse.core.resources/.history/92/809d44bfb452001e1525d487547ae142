package day21;
class MyThread2 implements Runnable{
	
	@Override
	public void run() {
		for(int i=0;i<500;i++) {
			System.out.println(i+"번째"+Thread.currentThread().getName());
		}
	}
	}

public class Thread02 {
	public static void main(String[] args) {
		/* Runnable을 구현하는 방법:
		 * 직접 객체 생성만으로 실행 불가능
		 * thread 클래스를 만들어서 생성자로 runnable 구현 객체를 추가해줘야함
		 * */

	


}
}
